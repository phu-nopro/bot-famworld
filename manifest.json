{
    "name": "Auto FW",
    "description": "Auto Farm World",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
        "64": "./images/farrmers-world-logo.png"
    },
    "action": {
      "default_title": "chan như con gián",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["https://play.farmersworld.io/*"],
        "js": ["./js/jquery.js","./js/myFam.js"],
        "css": ["./css/myCss.css"]
      }
    ]
  }